{
    "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
    },
    "plugins": ["prettier"],
    "extends": "airbnb-base",
    "overrides": [],
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "rules": {
      "camelcase": "off",
      "quotes": ["error", "double"],
      "no-unused-vars": "off",
      "no-console": "off",
      "func-names": "off",
      "no-process-exit": "off",
      "object-shorthand": "off",
      "class-methods-use-this": "off"
    }
  }
  